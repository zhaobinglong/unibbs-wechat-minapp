page {
	
	background: #fff;
}
.page_college {
	background-color: #FFF;
	text-align: center;
	display: flex;
	align-items: center;
	justify-content: center;
}
.page_college_name{
	font-size: 32rpx;
	font-weight:bold;
}
.page_college_logo{
	height:58rpx;
	width:58rpx;
	margin-right:10rpx;
}
.page_banner{
    overflow:hidden;
    background-color: #FFF;
}
.page_banner_type {
	margin: 25rpx;
}
.page_banner_type_item {
  display: inline-block;
  border: 2rpx solid #CCCCCC;
  width: 120rpx;
  height: 44rpx;
  font-size: 24rpx;
  text-align: center;
}
.page_banner_type_item {
  display: inline-block;
  border: 2rpx solid #CCCCCC;
  width: 120rpx;
  line-height: 44rpx;
  font-size: 24rpx;
  border-radius: 5rpx;
}
.page_banner_type_item_select {
  border: 2rpx solid #189AF8;
  background-color: #189AF8;
  color: #FFF
}

.page_list{
  width:50%;
  display:inline-block;
  vertical-align:top;
}
.page_list_no{
	color:#9f9f9f;
	background-color:#f0f0f0;
	position:absolute;
	top:0;
	right:0;
	bottom:0;
	left:0;
	text-align:center;
}
.page_list_no_cont{
	margin-top:50%;
}
.page_list_no_icon{
	width:152rpx;
	height:128rpx;
}
.page_list_left{
	padding:0 12rpx 0 24rpx;
}
.page_list_right{
	padding:0 24rpx 0 12rpx;
}
.page_list_city{
	line-height:80rpx;
	height:80rpx;
	text-align:center;
	font-size:28rpx;
	background-color:#000;
	justify-content:center;
	border-bottom:1rpx solid #666;
	color:#FFF;
}
.page_list_type{
    line-height:72rpx;
	background-color:#FEFEFE;
	width: 100%;
	white-space: nowrap;
	color:#808080;
	padding: 0 24rpx;
	padding-top: 92rpx;
}
.page_list_subcate {
}
.page_list_type_subcate {
   padding-left: 25%;
   display: flex;
   flex-wrap: wrap;
   align-content: space-between;
   font-size: 24rpx;
   color: #666;
}
.page_list_first {
  position: absolute;
  left: 0;
}
.page_list_type_subcate_name {
  display: inline-block;
  background-color: #efefef;
  line-height: 58rpx;
  width: 23%;
  text-align: center;
  font-size: 24rpx;
  padding: 0 10rpx;
  margin-bottom: 33rpx;
  margin-right: 10rpx;
  border-radius: 8rpx;
}
.page_list_type_item{
	line-height:52rpx;
	display:inline-block;
	font-size:28rpx;
	margin-right:40rpx;
}
.page_list_type_item_last {
	margin-right:10rpx;
}
.page_list_top_yesterday{
  background-color: rgba(0, 0, 0, 0.5);
  position:absolute;
  right:0;
  bottom:26rpx;
  padding:0 8rpx;
  color:#FFF;
  font-size:24rpx;
  line-height:40rpx;
}
.page_list_college{
	overflow:hidden;
    background-color:#FFFFFF; 
	font-size:24rpx;
	color:#666;
	height:104rpx;
    padding:0 24rpx;
    border-bottom:1rpx solid  #e6e6e6;
}
.page_list_college_img{
	width:100%;
}

.page_list_college_member{
	width:56rpx;
	height:56rpx;
	border-radius:50%;

}

.page_list_item{
	background-color:#FFF;
	box-shadow:0 1px 2px 0 rgba(0,0,0,.1);
	border-radius:4rpx;
	margin-top:16rpx;
	box-shadow: 0 0 8rpx 0  rgba(207, 207, 207, 0.5);
	overflow:hidden;
	font-size:0;
}
.page_list_item_bottom{
	padding:18rpx 16rpx 12rpx 16rpx;
}
.page_list_item_price{
	color:#f5a623;
}
.page_list_img{
	width:100%;
	font-size:0;
}
.page_list_cont{
	color:#666;
	margin:10rpx;
	font-size:28rpx;
	display: -webkit-box;
	-webkit-box-orient: vertical;
	-webkit-line-clamp: 3;
	overflow: hidden;
	word-wrap: break-word; 
	word-break: normal; 
}
.page_list_user{
	color:#666;
	padding:10rpx;
	font-size:28rpx;	
}

.page_list_headimg{
	width:40rpx;
	height:40rpx;
	border-radius:50%;
	margin-right:12rpx;
}
.scroll-view-item{
	line-height:100rpx;
	/*width:60rpx;*/
	display:inline-block;
	font-size:28rpx;
	padding:0 20rpx;
}
.list_type_item_select{
   color:#2c2c2c;
   font-weight: 600;
   border-bottom: 4rpx solid  #189AF8;
   border-radius: 4rpx;
   box-sizing:border-box;
}
.list_head_right{
	text-align:right;
}
.list_old_price{
	font-size:24rpx;
	color:#8a8a8a;
	text-decoration:line-through
}
.list_price{
  color:#d56455;
  font-size:50rpx;
  line-height:30rpx;
  font-weight:500;
}
.list_phone{

  border:1rpx solid #65b997;
  font-size:24rpx;
  color:#65b997;
  padding:4rpx 6rpx;
  border-radius:6rpx;
}
.button{
	background-color:#65b997;
	color:#FFF;
	display:inline-block;
	padding:0 60rpx;
	margin:0 auto;
	margin-top:200rpx;
}

.page_search{
	padding:0 36rpx 20rpx;
/*	border-top:1px solid #e6e6e6;
	border-bottom:1px solid #e6e6e6;*/
	background-color: #FFF;

}
.page_search_input {
	background: #f5f5f5;
	border-radius: 36rpx;
	width: 100%;
	height: 60rpx;
	overflow: hidden;
	padding: 0 23rpx;
}



